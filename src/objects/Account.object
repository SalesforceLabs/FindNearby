<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <fields>
        <fullName>Find_Nearby__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL( Mapping_Status__c  , &apos;Located&apos;), HYPERLINK( &quot;apex/FindNearbymap?lid=null&amp;aid=&quot; &amp; Id,&quot;Map it!&quot;,&quot;_parent&quot;) , HYPERLINK( &quot;apex/AGeoCode?id=&quot; &amp; Id,&quot;Locate Account&quot;,&quot;_parent&quot;) )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Find Nearby</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Lat__c</fullName>
        <externalId>false</externalId>
        <label>Latitude</label>
        <precision>7</precision>
        <required>false</required>
        <scale>2</scale>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Lon__c</fullName>
        <externalId>false</externalId>
        <label>Longitude</label>
        <precision>7</precision>
        <required>false</required>
        <scale>2</scale>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Mapping_Status__c</fullName>
        <externalId>false</externalId>
        <label>Mapping Status</label>
        <picklist>
            <picklistValues>
                <fullName>Not Located Yet</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Located</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Problem with Address</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Bad Google Key</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Google Exhausted</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Picklist</type>
    </fields>
    <fields>
        <fullName>Which_Address__c</fullName>
        <externalId>false</externalId>
        <label>Mapping Address</label>
        <picklist>
            <picklistValues>
                <fullName>Billing</fullName>
                <default>true</default>
            </picklistValues>
            <picklistValues>
                <fullName>Shipping</fullName>
                <default>false</default>
            </picklistValues>
            <picklistValues>
                <fullName>Do Not Map</fullName>
                <default>false</default>
            </picklistValues>
            <sorted>false</sorted>
        </picklist>
        <trackFeedHistory>false</trackFeedHistory>
        <type>Picklist</type>
    </fields>
    <webLinks>
        <fullName>Map_Accounts</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Map Accounts</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/10.0/connection.js&quot;)}
var selecteditems={!GETRECORDIDS($ObjectType.Account)}; 
window.location= &quot;/apex/FindNearbymap?alids=&quot;+selecteditems;</url>
    </webLinks>
</CustomObject>
